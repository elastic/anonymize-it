{
  "source": {
    "type": "elasticsearch",
    "params": {
      "host": "host:port",
      "index": "you-index-pattern-*",
      "use_ssl": false,
      "auth": "api",
      "query": {
        "match": {
          "username": "blaklaybul"
        }
      }
    }
